<?xml version='1.0' encoding='utf-8'?>
<widget id="co.geomati.offlinevectormap" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Offline Vector Map</name>
    <description>
        A sample MapboxGL map that works completely offline.
    </description>
    <author email="micho.garcia@geomati.co" href="http://geomati.co">
        The geomati.co team
    </author>
    <icon density="mdpi" src="res/mdpi.png" />
    <icon density="hdpi" src="res/hdpi.png" />
    <icon density="xhdpi" src="res/xhdpi.png" />
    <icon density="xxhdpi" src="res/xxhdpi.png" />
    <icon density="xxxhdpi" src="res/xxxhdpi.png" />
    <content src="http://172.27.203.112:8080/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.2.2" />
    <plugin name="cordova-plugin-file" spec="~4.3.2" />
    <plugin name="cordova-plugin-sqlite-2" spec="^1.0.4" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
